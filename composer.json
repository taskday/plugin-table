{
    "name": "taskday/plugin-table",
    "description": "",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "nunomaduro/collision": "^4.2",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Performing\\Taskday\\Table\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Performing\\Taskday\\Table\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Performing\\Taskday\\Table\\TableServiceProvider"
            ]
        }
    }
}
